@model IEnumerable<Publicaciones.Web.Models.RoySchedViewResult>

@{
    ViewData["Title"] = "Index";
}

<h1>RoyScheds</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.royalty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.loRange)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.hiRange)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.royalty)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.loRange)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hiRange)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.roySchedID }) |
                @Html.ActionLink("Details", "Details", new { id=item.roySchedID })
            </td>
        </tr>
}
    </tbody>
</table>
